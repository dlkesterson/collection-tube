{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/config/*": [
        "config/*"
      ],
      "@/lib/*": [
        "lib/*"
      ],
      "@/api/*": [
        "pages/api/*"
      ],
      "@/models/*": [
        "models/*"
      ],
      "@/db*": [
        "db/index"
      ]
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "esnext",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "isolatedModules": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "models/channel.js",
    "models/index.js",
    "db/extra-setup.js",
    "db/index.ts",
    "server.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
